{"html": "<form method='post' action={% url 'save_outfit_to_schedule' %}><input type='hidden' name='schedule_day' value='{{ day }}'><div class='row text-center'><div class = 'col-md-6 thumnail text-center'><div class='row my-outfit'>{% if top1  %}<input type='hidden' name='top1' value='{{ top1.id }}'><div class = 'col-md-6 thumnail'><img  src={% url 'get_image' top1.id%} alt='{{ top1.name }}' class='clothes-img outfit_top1' id = 'clothes-top1-{{top1.id}}'/></div>{% endif %}    {% if top2 %} <input type='hidden' name='top2' value='{{ top2.id }}'>   <div class = 'col-md-6 thumnail'><img  src={% url 'get_image' top2.id%} alt='{{ top2.name }}' class='clothes-img outfit_top2' id = 'clothes-top2-{{top2.id}}'/></div>    {% endif %}{% if dress %}<input type='hidden' name='dress' value='{{ dress.id }}'>        <div class = 'col-md-6 thumnail'><img  src={% url 'get_image' dress.id%} alt='{{ dress.name }}' class='clothes-img outfit_dress' id = 'clothes-dress-{{dress.id}}'/></div>    {% endif %}       {% if bottom %}  <input type='hidden' name='bottom' value='{{ bottom.id }}'>      <div class = 'col-md-6 thumnail'><img  src={% url 'get_image' bottom.id%} alt='{{ bottom.name }}' class='clothes-img outfit_bottom' id = 'clothes-bottom-{{bottom.id}}'/></div>    {% endif %}        {% if shoes %} <input type='hidden' name='shoes' value='{{ shoes.id }}'>       <div class = 'col-md-6 thumnail'><img  src={% url 'get_image' shoes.id%} alt='{{ shoes.name }}' class='clothes-img outfit_shoes' id = 'clothes-shoes-{{shoes.id}}'/></div>    {% endif %}  </div>   <div class = 'text-center' style = 'margin-top:10px;'><button type = 'submit' class='btn btn-primary' id='saveoutfit2'>Save Outfit</button></div>{% csrf_token %}</form>"}